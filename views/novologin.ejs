<html>
    <head>
        <title>Quest - Cadastro</title>
    </head>
    <body>
        <form action=""> 
            <h1>Quest</h1>
            <h2>Cadastro</h2>
            Nome do Jogador: 
            <input type="text" id="nome">
            <br><br>
            Login (e-mail): 
            <input type="email" id="email">
            <br><br>
            Senha: 
            <input type="password" id="password">
            <br><br>
            <input type="submit" value="Criar Jogador">
            <input type="reset" value="Limpar">
            <input type="button" value="Voltar" id="voltar">
        </form>
    </body>
    <script  
        src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"
    >
    </script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
    <script>
        const botaoNovoJogador = document.getElementById("voltar")
        botaoNovoJogador.addEventListener('click', voltar)
        function voltar() {
            location.href="/"
        }
    </script>
</html>